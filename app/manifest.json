{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/pokeball-16.png",
    "128": "images/pokeball-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "../node_modules/superagent/superagent.js",
      "scripts/constants.js",
      "scripts/utils.js",
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "background",
    "cookies",
    "notifications",
    "geolocation",
    "webNavigation",
    "https://pokevision.com/*",
    "webRequest",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/pokeball-19.png",
      "38": "images/pokeball-38.png"
    },
    "default_title": "Pokemon GO Notifications",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/pokeball-16.png",
    "images/pokemon/*.png",
    "styles/mdl/*.*",
    "styles/mdl/font/*.*"
  ]
}
